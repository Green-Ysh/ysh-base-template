{
  "name": "ysh-base-template",
  "private": true,
  "version": "1.0.0",
  "description": "A Vue 3 template with Vite, Ant Design Vue, and more essential tools for rapid development",
  "author": "CoderYsh",
  "license": "MIT",
  "keywords": [
    "vue3",
    "vite",
    "ant-design-vue",
    "template",
    "typescript",
    "pinia",
    "vue-router"
  ],
  "homepage": "https://github.com/CoderYsh/ysh-base-template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoderYsh/ysh-base-template.git"
  },
  "bugs": {
    "url": "https://github.com/CoderYsh/ysh-base-template/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:style": "stylelint \"src/**/*.{css,scss,vue}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,vue,ts,tsx,json,css,scss,less,md}\"",
    "prepare": "husky install",
    "commit": "git-cz",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "build:prod": "npm run clean && vite build --mode production",
    "build:dev": "npm run clean && vite build --mode development",
    "analyze": "vite build --mode production --config vite.config.analyze.js",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@vueuse/core": "^11.3.0",
    "ant-design-vue": "^3.2.15",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "lodash-es": "^4.17.21",
    "pinia": "^2.2.6",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@-ysh-/browserslist-config": "^1.1.0",
    "@-ysh-/code-specification-unid": "^1.2.3",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/test-utils": "^2.4.4",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.31.0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "postcss-html": "^1.6.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.81.0",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.18.5",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.10",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.vue": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write"
    ],
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css, scss}": [
      "stylelint --fix=false",
      "prettier --write"
    ]
  }
}
